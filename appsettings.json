{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Warning"
      }
    },
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithEnvironmentName",
      "WithEnvironmentUserName",
      "WithClientIp",
      "WithClientAgent",
      "WithThreadId"
    ],
    "WriteTo": [
      {
        "Name": "Elasticsearch",
        "Args": {
          "serverUrl": "http://localhost:9200"
        }
      }
    ]
  },
  "ElasticConfiguration": {
    "Uri": "http://localhost:9200"
  },
  "AllowedHosts": "*"
}